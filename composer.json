{
    "name": "corentincrz/hello-world",
    "type": "symfony-bundle",
    "description": "Hello world composer package",
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "authors": [
        {
            "name": "CorentinCrz",
            "email": "ccroiz@live.fr"
        }
    ],
    "require": {
        "php": "^7.4",
        "symfony/http-kernel": "^4.4",
        "doctrine/collections": "^1",
        "symfony/dependency-injection": "^4.4",
        "symfony/config": "^4.4",
        "symfony/validator": "^4.4",
        "symfony/serializer": "^4.4",
        "symfony/property-info": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "HelloWorld\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HelloWorld\\Tests\\": "tests/"
        }
    }
}
